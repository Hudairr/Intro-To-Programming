~~~~~~ M O N T H S   O F   T H E   Y E A R ~~~~~~
#include<iostream>
#include <array>
#include <string>
using namespace std;
int main()
{
	string month[12] = { "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" };
  // Declaring & Initialising only
	return 0;  
}

~~~~~~ T I M E   T R A V E L ~~~~~~
#include<iostream>
#include <array>
#include <string>
using namespace std;
int main()
{
	string month[12] = { "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" };
	for (int i = 0; i < 12; i++) {
		cout << month[i] << endl;
	}
	return 0;
}

~~~~~~ M A R K S ~~~~~~
#include<iostream>
#include <array>
using namespace std;
int main()
{
	int m[5];
	cout << "Enter marks of five subjects: " << endl;
	for (int i = 0; i < 5; i++) {
		cin >> m[i];
	}
	double avg = (m[0] + m[1] + m[2] + m[3] + m[4]) / 5;
	cout << "The average is: " << avg;

	return 0;
}
